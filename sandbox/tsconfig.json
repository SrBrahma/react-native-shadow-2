{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Expo",
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "incremental": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,    /* Include 'undefined' in index signature results */
    "useUnknownInCatchVariables": false,
    "allowJs": true
  },
  "include": ["App.tsx", "src/**/*"],
  "exclude": ["node_modules/**/*", "*.json", ".*"]
}
